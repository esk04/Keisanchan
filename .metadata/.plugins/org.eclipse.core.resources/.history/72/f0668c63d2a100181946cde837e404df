package com.internousdev.keisanchan.action;

import com.opensymphony.xwork2.ActionSupport;

public class KeisanAction extends ActionSupport{

	private number toki;
	private number zikoshi;
	private number keijo;
	private number uriage;
	private number uriage_sori;
	private number eigyo;
	private number total_assets;
	private number uriage_saiken;
	private number uriage_genka;
	private number tana;
	private number ryudo_assets;
	private number ryudo_fusai;
	private number kotei_assets;
	private number total_fusai;

	private number roe = 0d;
	private number roa = 0d;
	private number uriage_keijo_ratio = 0d;
	private number uriage_sori_ratio = 0d;
	private number uriage_eigyo_ratio = 0d;

	private number total_kaiten_ratio = 0d;
	private number zaiko_kaiten_ratio = 0d;

	private number ryudo_ratio = 0d;
	private number zikoshi_ratio = 0d;
	private number kotei_ratio = 0d;
	private number fusai_ratio = 0d;

		public String execute(){
		 roe = toki/zikoshi;
		 roa = toki/total_assets;
		 uriage_keijo_ratio = keijo/uriage;
		 uriage_sori_ratio = uriage_sori/uriage;
		 uriage_eigyo_ratio = eigyo/uriage;

		 total_kaiten_ratio = uriage/total_assets;
		 zaiko_kaiten_ratio = uriage_genka/tana;

		 ryudo_ratio = ryudo_assets/ryudo_fusai;
		 zikoshi_ratio = zikoshi/total_assets;
		 kotei_ratio = kotei_assets/zikoshi;
		 fusai_ratio = total_fusai/zikoshi;
		 return SUCCESS;
	}


