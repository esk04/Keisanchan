	@SuppressWarnings("unused")
	public String execute()throws Exception{
		number roe = toki/zikoshi;
		number roa = toki/total_assets;
		number uriage_keijo_ratio = keijo/uriage;
		number uriage_sori_ratio = uriage_sori/uriage;
		number uriage_eigyo_ratio = eigyo/uriage;

		number total_kaiten_ratio = uriage/total_assets;
		number zaiko_kaiten_ratio = uriage_genka/tana;

		number ryudo_ratio = ryudo_assets/ryudo_fusai;
		number zikoshi_ratio = zikoshi/total_assets;
		number kotei_ratio = kotei_assets/zikoshi;
		number fusai_ratio = total_fusai/zikoshi;
		return null;
	}


	public number getToki(){
		return this.toki;
	}
	public number getZikoshi(){
		return this.zikoshi;
	}
	public number getKeijo(){
		return this.keijo;
	}
	public number getUriage(){
		return this.uriage;
	}
	public number getUriage_sori(){
