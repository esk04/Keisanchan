package com.internousdev.keisanchan.action;

import com.opensymphony.xwork2.ActionSupport;

public class KeisanAction extends ActionSupport{
	private String company_name;

	private String toki;
	private String zikoshi;
	private String keijo;
	private String uriage;
	private String uriage_sori;
	private String eigyo;
	private String total_assets;
	private String uriage_saiken;
	private String uriage_genka;
	private String tana;
	private String ryudo_assets;
	private String ryudo_fusai;
	private String kotei_assets;
	private String total_fusai;


	private String roe;
	private String roa;
	private String uriage_keijo_ratio;
	private String uriage_sori_ratio;
	private String uriage_eigyo_ratio;

	private String total_kaiten_ratio ;
	private String zaiko_kaiten_ratio;

	private String ryudo_ratio ;
	private String zikoshi_ratio;
	private String kotei_ratio ;
	private String fusai_ratio ;

		public String execute(){
		 roe = toki/zikoshi;
		 roa = toki/total_assets;
		 uriage_keijo_ratio = keijo/uriage;
		 uriage_sori_ratio = uriage_sori/uriage;
		 uriage_eigyo_ratio = eigyo/uriage;

		 total_kaiten_ratio = uriage/total_assets;
		 zaiko_kaiten_ratio = uriage_genka/tana;

		 ryudo_ratio = ryudo_assets/ryudo_fusai;
		 zikoshi_ratio = zikoshi/total_assets;
		 kotei_ratio = kotei_assets/zikoshi;
		 fusai_ratio = total_fusai/zikoshi;
		 return SUCCESS;
	}

		public String getToki() {
			return toki;
		}

		public void setToki(String toki) {
			this.toki = toki;
		}

		public String getZikoshi() {
			return zikoshi;
		}

		public void setZikoshi(String zikoshi) {
			this.zikoshi = zikoshi;
		}

		public String getKeijo() {
			return keijo;
		}

		public void setKeijo(String keijo) {
			this.keijo = keijo;
		}

		public String getUriage() {
			return uriage;
		}

		public void setUriage(String uriage) {
			this.uriage = uriage;
		}

		public String getUriage_sori() {
			return uriage_sori;
		}

		public void setUriage_sori(String uriage_sori) {
			this.uriage_sori = uriage_sori;
		}

		public String getEigyo() {
			return eigyo;
		}

		public void setEigyo(String eigyo) {
			this.eigyo = eigyo;
		}

		public String getTotal_assets() {
			return total_assets;
		}

		public void setTotal_assets(String total_assets) {
			this.total_assets = total_assets;
		}

		public String getUriage_saiken() {
			return uriage_saiken;
		}

		public void setUriage_saiken(String uriage_saiken) {
			this.uriage_saiken = uriage_saiken;
		}

		public String getUriage_genka() {
			return uriage_genka;
		}

		public void setUriage_genka(String uriage_genka) {
			this.uriage_genka = uriage_genka;
		}

		public String getTana() {
			return tana;
		}

		public void setTana(String tana) {
			this.tana = tana;
		}

		public String getRyudo_assets() {
			return ryudo_assets;
		}

		public void setRyudo_assets(String ryudo_assets) {
			this.ryudo_assets = ryudo_assets;
		}

		public String getRyudo_fusai() {
			return ryudo_fusai;
		}

		public void setRyudo_fusai(String ryudo_fusai) {
			this.ryudo_fusai = ryudo_fusai;
		}

		public String getKotei_assets() {
			return kotei_assets;
		}

		public void setKotei_assets(String kotei_assets) {
			this.kotei_assets = kotei_assets;
		}

		public String getTotal_fusai() {
			return total_fusai;
		}

		public void setTotal_fusai(String total_fusai) {
			this.total_fusai = total_fusai;
		}

		public String getRoe() {
			return roe;
		}

		public void setRoe(String roe) {
			this.roe = roe;
		}

		public String getRoa() {
			return roa;
		}

		public void setRoa(String roa) {
			this.roa = roa;
		}

		public String getUriage_keijo_ratio() {
			return uriage_keijo_ratio;
		}

		public void setUriage_keijo_ratio(String uriage_keijo_ratio) {
			this.uriage_keijo_ratio = uriage_keijo_ratio;
		}

		public String getUriage_sori_ratio() {
			return uriage_sori_ratio;
		}

		public void setUriage_sori_ratio(String uriage_sori_ratio) {
			this.uriage_sori_ratio = uriage_sori_ratio;
		}

		public String getUriage_eigyo_ratio() {
			return uriage_eigyo_ratio;
		}

		public void setUriage_eigyo_ratio(String uriage_eigyo_ratio) {
			this.uriage_eigyo_ratio = uriage_eigyo_ratio;
		}

		public String getTotal_kaiten_ratio() {
			return total_kaiten_ratio;
		}

		public void setTotal_kaiten_ratio(String total_kaiten_ratio) {
			this.total_kaiten_ratio = total_kaiten_ratio;
		}

		public String getZaiko_kaiten_ratio() {
			return zaiko_kaiten_ratio;
		}

		public void setZaiko_kaiten_ratio(String zaiko_kaiten_ratio) {
			this.zaiko_kaiten_ratio = zaiko_kaiten_ratio;
		}

		public String getRyudo_ratio() {
			return ryudo_ratio;
		}

		public void setRyudo_ratio(String ryudo_ratio) {
			this.ryudo_ratio = ryudo_ratio;
		}

		public String getZikoshi_ratio() {
			return zikoshi_ratio;
		}

		public void setZikoshi_ratio(String zikoshi_ratio) {
			this.zikoshi_ratio = zikoshi_ratio;
		}

		public String getKotei_ratio() {
			return kotei_ratio;
		}

		public void setKotei_ratio(String kotei_ratio) {
			this.kotei_ratio = kotei_ratio;
		}

		public String getFusai_ratio() {
			return fusai_ratio;
		}

		public void setFusai_ratio(String fusai_ratio) {
			this.fusai_ratio = fusai_ratio;
		}

		public String getCompany_name() {
			return company_name;
		}

		public void setCompany_name(String company_name) {
			this.company_name = company_name;
		}



}